{
  "name": "spartan-v4",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest --config jest.config.cjs",
    "test:e2e": "jest --config jest.config.cjs e2e/",
    "test:performance": "jest --config jest.config.cjs performance/",
    "test:visual": "jest --config jest.config.cjs visual/",
    "test:integration": "jest --config jest.config.cjs integration/",
    "test:load": "jest --config jest.config.cjs load-testing/",
    "test:all-advanced": "npm run test:performance && npm run test:integration && npm run test:load",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build:prod": "NODE_ENV=production npm run build",
    "docker:build": "docker build -t spartan4 .",
    "docker:run": "docker run -p 3000:80 spartan4",
    "docker:compose": "docker-compose up --build"
  },
  "dependencies": {
    "@google/genai": "^1.17.0",
    "@supabase/supabase-js": "^2.57.4",
    "dotenv": "^17.2.2",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^2.12.7",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.14.0",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.3.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-image-snapshot": "^6.5.1",
    "postcss": "^8.5.6",
    "puppeteer": "^24.22.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.13",
    "terser": "^5.44.0",
    "ts-jest": "^29.1.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-imagemin": "^0.6.1"
  }
}
