{
  "name": "spartan-v4",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "node scripts/verify-env.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest --config jest.config.cjs",
    "test:e2e": "jest --config jest.config.cjs e2e/",
    "test:performance": "jest --config jest.config.cjs performance/",
    "test:visual": "jest --config jest.config.cjs visual/",
    "test:integration": "jest --config jest.config.cjs integration/",
    "test:load": "jest --config jest.config.cjs load-testing/",
    "test:all-advanced": "npm run test:performance && npm run test:integration && npm run test:load",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build:prod": "NODE_ENV=production npm run build",
    "docker:build": "docker build -t spartan4 .",
    "docker:run": "docker run -p 3000:80 spartan4",
    "docker:compose": "docker-compose up --build"
  },
  "dependencies": {
    "@google/generative-ai": "^0.12.0",
    "@supabase/supabase-js": "^2.57.4",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.11.17",
    "lucide-react": "^0.453.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.13.3",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/supertest": "^6.0.2",
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.2.0",
    "imagemin-gifsicle": "^5.2.0",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^5.0.1",
    "imagemin-svgo": "^11.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-image-snapshot": "^6.4.0",
    "postcss": "^8.5.6",
    "puppeteer": "^23.9.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.36.0",
    "ts-jest": "^29.2.5",
    "typescript": "~5.6.3",
    "vite": "^7.1.6",
    "vite-plugin-imagemin": "^0.4.0"
  }
}
